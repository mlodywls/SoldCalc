<Window x:Class="SoldCalc.Wind_of_Html"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SoldCalc"
        mc:Ignorable="d"  
        Background="Transparent"    
        Title="WinCalc - Podgląd PDF" Height="1200" Width="800"      
        Loaded="Window_Loaded" Unloaded="Window_Unloaded"
    ResizeMode="CanResizeWithGrip"
    WindowState="Normal" WindowStartupLocation="CenterOwner" >
    <Grid Margin="0,0,20,0">
        <TextBlock x:Name="Serhtext" Margin="0,0,0,5"/>
        <StackPanel Orientation ="Horizontal" VerticalAlignment="Top" Margin="0,10,0,20" x:Name="PanelNaw" SizeChanged="PanelNaw_SizeChanged" />
        <DockPanel x:Name="AddWeb" />
    </Grid>
</Window>

